{
"manifest_version": 3,

"name": "IMDB custom search",
"description": "Gets search suggestions from IMDB directly.",
"version": "1.4",

"background": { "service_worker": "background.js" },

"omnibox": { "keyword": "imdb" },

"icons": {
    "16": "icon16.png",
    "24": "icon24.png",
    "32": "icon32.png",
    "48": "icon48.png"
},
"host_permissions": [ "*://sg.media-imdb.com/suggests/*" ]
}
