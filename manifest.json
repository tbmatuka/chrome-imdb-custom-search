{
"manifest_version": 2,

"name": "IMDB custom search",
"description": "Gets search suggestions from IMDB directly.",
"version": "1.3",

"background": { "page": "background.html" },

"omnibox": { "keyword": "imdb" },

"icons": {
	"16": "icon16.png",
	"24": "icon24.png",
	"32": "icon32.png",
	"48": "icon48.png"
	},

"permissions": [ "*://sg.media-imdb.com/suggests/*" ]
}
